    {
      "cell_type": "code",
      "source": [
        "number = int(input(\"enter the number :\"))\n",
        "for i in range(number):\n",
        "  for j in range(number-i):\n",
        "    print(\"*\" , end=\" \")\n",
        "  print(\"\")"
